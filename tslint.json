{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "no-duplicate-variable": true,
        "no-unused-variable": true,
        "semicolon": [
            true,
            "always"
        ],
        "no-console": true,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ]
    }
}
